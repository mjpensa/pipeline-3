â”‚         x: xPos + 0.15, y: 2.9, w: 2.53, h: 0.5,                            â”‚
â”‚         fontSize: 16, fontFace: 'Calibri', color: '1a365d', bold: true      â”‚
â”‚     });                                                                      â”‚
â”‚     // Step description                                                      â”‚
â”‚     slide.addText(step.desc, {                                              â”‚
â”‚         x: xPos + 0.15, y: 3.5, w: 2.53, h: 2.5,                            â”‚
â”‚         fontSize: 12, fontFace: 'Calibri', color: '718096', valign: 'top'   â”‚
â”‚     });                                                                      â”‚
â”‚ });                                                                          â”‚
